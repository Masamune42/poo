package fr.eni.ecoleeni.oop;

public class Boat {
	// instance variable
	// ==attribute
	// ==property
	// ==field
	String name;
	int size;

	public Boat() {
		System.out.println("Creating new instance !");

	}

	// méthodes d'instance
	void goForward() {
		System.out.println("Go forward !");
	}

	void sink() {
		System.out.println("Glouglou!");
	}

}
