package fr.eni.ecoleeni.oop.model;

public class Captain {
	public String name;
	public int age;
	public Boat boat;
	
	public Captain() {
		
	}

	public Captain(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
	public void getDrunk() {
		System.out.println("Getting drunk AF!");
	}
	
	

}
